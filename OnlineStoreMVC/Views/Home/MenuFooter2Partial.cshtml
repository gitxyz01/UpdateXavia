@model List<OnlineStore.Model.Context.Menu>

<ul class="toggle-footer" style="">
    @if (Model.Count > 0)
    {
        foreach (var item in Model)
        {
            <li class="item">
                <a style="text-decoration:none;" href="@item.Link" title="@item.Text">
                    @item.Text
                </a>
            </li>
        }
    }
    else
    {
        <li>
            <a style="text-decoration:none;" href="#" title="Trống">
                Trống
            </a>
        </li>
    }
</ul>



